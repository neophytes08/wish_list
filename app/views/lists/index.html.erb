<div class="row">
  <div class="col s12 m6 l4" ng-repeat="list in wishList">
    <div class="card">
      <div class="card-image waves-effect waves-block waves-light">
        <img src="{{list.image}}" class="activator">
      </div>
      <div class="card-content">
        <span class="card-title activator grey-text text-darken-4">{{list.list_name}} <i class="mdi-navigation-more-vert right"></i></span>
        <p><a href class="btn-flat waves-effect waves-blue" ng-click="editList(list)">Edit</a> <a href class="btn-flat waves-effect waves-blue" ng-click="deleteList(list)">Delete</a> </p>
      </div>
      <div class="card-reveal">
        <span class="card-title grey-text text-darken-4">{{list.list_name}} <i class="mdi-navigation-close right"></i></span>
        <p>{{list.list_description}}</p>
      </div>
    </div>
  </div>
</div>